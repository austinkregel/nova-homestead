{
    "name": "kregel/homestead",
    "description": "A Laravel Nova tool.",
    "keywords": [
        "laravel",
        "nova"
    ],
    "license": "MIT",
    "require": {
        "php": ">=7.1.0",
        "illuminate/database": ">=5.8",
        "illuminate/support": ">=5.8",
        "phpunit/phpunit": "^8.5"
    },
    "autoload": {
        "psr-4": {
            "Kregel\\Homestead\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kregel\\Homestead\\ToolServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/framework": "5.8.*"
    }
}
